{
  "name": "ng-elements-starter",
  "version": "1.0.0",
  "private": true,
  "description": "Angular Elements NodeJS Widget Server Starter",
  "repository": "https://github.com/rfprod/ng-elements-starter",
  "license": "MIT",
  "author": "rfprod",
  "scripts": {
    "analyze:app": "npm run build:app:stats; npx webpack-bundle-analyzer ./dist/ng-elements-app/stats.json",
    "analyze:balance": "npm run build:balance:stats; npx webpack-bundle-analyzer ./dist/ng-elements-balance/stats.json",
    "analyze:catalog": "npm run build:catalog:stats; npx webpack-bundle-analyzer ./dist/ng-elements-catalog/stats.json",
    "analyze:orders": "npm run build:orders:stats; npx webpack-bundle-analyzer ./dist/ng-elements-orders/stats.json",
    "analyze:passport": "npm run build:passport:stats; npx webpack-bundle-analyzer ./dist/ng-elements-passport/stats.json",
    "build:all:express": "bash shell/build.sh express",
    "build:all:firebase": "bash shell/build.sh firebase",
    "build:app:prod": "ng build --project ng-elements-app --prod",
    "build:app:prod:firebase": "ng build --project ng-elements-app --prod --baseHref /ng-elements-app/",
    "build:app:stats": "ng build --stats-json --project ng-elements-app --configuration analyze-bundle",
    "build:balance:prod": "ng build --project ng-elements-balance --prod && npx npm-run-all -s generate:bootstrap-script:balance generate:widget-example:balance",
    "build:balance:stats": "ng build --stats-json --project ng-elements-balance --configuration analyze-bundle",
    "build:catalog:prod": "ng build --project ng-elements-catalog --prod && npx npm-run-all -s generate:bootstrap-script:catalog generate:widget-example:catalog",
    "build:catalog:stats": "ng build --stats-json --project ng-elements-catalog --configuration analyze-bundle",
    "build:orders:prod": "ng build --project ng-elements-orders --prod && npx npm-run-all -s generate:bootstrap-script:orders generate:widget-example:orders",
    "build:orders:stats": "ng build --stats-json --project ng-elements-orders --configuration analyze-bundle",
    "build:passport:prod": "ng build --project ng-elements-passport --prod && npx npm-run-all -s generate:bootstrap-script:passport generate:widget-example:passport",
    "build:passport:stats": "ng build --stats-json --project ng-elements-passport --configuration analyze-bundle",
    "compodoc:generate": "compodoc -p src/tsconfig.app.json",
    "compodoc:generate:report-to-dist": "compodoc -p src/tsconfig.app.json && cp -r ./documentation ./dist/documentation",
    "compodoc:serve": "compodoc -p src/tsconfig.app.json -s",
    "e2e": "ng e2e",
    "e2e:headless": "ng e2e --headless --watch=false",
    "generate:bootstrap-script:balance": "bash shell/generate-widget-bootstrap-script.sh dist/ng-elements-balance ng-elements-balance app-balance-widget",
    "generate:bootstrap-script:catalog": "bash shell/generate-widget-bootstrap-script.sh dist/ng-elements-catalog ng-elements-catalog app-catalog-widget",
    "generate:bootstrap-script:orders": "bash shell/generate-widget-bootstrap-script.sh dist/ng-elements-orders ng-elements-orders app-orders-widget",
    "generate:bootstrap-script:passport": "bash shell/generate-widget-bootstrap-script.sh dist/ng-elements-passport ng-elements-passport app-passport-widget",
    "generate:widget-example:all": "npx npm-run-all -s generate:widget-example:balance generate:widget-example:catalog generate:widget-example:orders generate:widget-example:passport",
    "generate:widget-example:all:local": "npx npm-run-all -s generate:widget-example:balance:local generate:widget-example:catalog:local generate:widget-example:orders:local generate:widget-example:passport:local",
    "generate:widget-example:balance": "bash shell/generate-widget-usage-example.sh balance",
    "generate:widget-example:balance:local": "bash shell/generate-widget-usage-example.sh balance local",
    "generate:widget-example:catalog": "bash shell/generate-widget-usage-example.sh catalog",
    "generate:widget-example:catalog:local": "bash shell/generate-widget-usage-example.sh catalog local",
    "generate:widget-example:orders": "bash shell/generate-widget-usage-example.sh orders",
    "generate:widget-example:orders:local": "bash shell/generate-widget-usage-example.sh orders local",
    "generate:widget-example:passport": "bash shell/generate-widget-usage-example.sh passport",
    "generate:widget-example:passport:local": "bash shell/generate-widget-usage-example.sh passport local",
    "postinstall": "npx sort-package-json && npm run ngcc",
    "install:global": "bash shell/install.sh global",
    "install:project": "bash shell/install.sh project",
    "lint": "ng lint",
    "ng": "ng",
    "ngcc": "ngcc --tsconfig tsconfig.base.json",
    "precommit:lint": "bash shell/precommit.sh lint",
    "prettier:html": "npx prettier -c src/**/*.html",
    "prettier:html:fix": "npx prettier -c --write src/**/*.html",
    "serve:app": "npm run start:server & ng serve --project ng-elements-app",
    "serve:balance": "npm run start:server & ng serve --project ng-elements-balance",
    "serve:catalog": "npm run start:server & ng serve --project ng-elements-catalog",
    "serve:orders": "npm run start:server & ng serve --project ng-elements-orders",
    "serve:passport": "npm run start:server & ng serve --project ng-elements-passport",
    "preserver": "npm run build",
    "server": "node server.js",
    "start": "node server.js dev & ng serve --project ng-elements-app",
    "start:prod": "node server.js dev & ng serve --project ng-elements-app --prod",
    "start:server": "node server.js",
    "stylelint": "npx stylelint \"./src/**/*.scss\"",
    "stylelint:fix": "npx stylelint \"./src/**/*.scss\" --fix",
    "test": "ng test",
    "test:single-run": "ng test ng-elements-app --watch=false --coverage=false",
    "test:single-run:report": "ng test ng-elements-app --watch=false",
    "test:single-run:report-to-dist": "ng test ng-elements-app --watch=false && cp -r ./coverage ./dist/coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit:lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@angular/animations": "10.0.4",
    "@angular/cdk": "10.0.2",
    "@angular/common": "10.0.4",
    "@angular/compiler": "10.0.4",
    "@angular/core": "10.0.4",
    "@angular/elements": "10.0.4",
    "@angular/flex-layout": "10.0.0-beta.32",
    "@angular/forms": "10.0.4",
    "@angular/material": "10.0.2",
    "@angular/material-moment-adapter": "10.0.2",
    "@angular/platform-browser": "10.0.4",
    "@angular/platform-browser-dynamic": "10.0.4",
    "@angular/pwa": "0.1000.3",
    "@angular/router": "10.0.4",
    "@angular/service-worker": "10.0.4",
    "@ngxs/form-plugin": "3.6.2",
    "@ngxs/router-plugin": "^3.6.2",
    "@ngxs/store": "3.6.2",
    "@webcomponents/custom-elements": "1.4.1",
    "@webcomponents/webcomponentsjs": "2.4.3",
    "body-parser": "1.19.0",
    "components-font-awesome": "5.9.0",
    "compression": "1.7.4",
    "core-js": "3.6.5",
    "express": "4.17.1",
    "hammerjs": "2.0.8",
    "jwt-simple": "0.5.6",
    "marked": "1.1.1",
    "material-design-icon-fonts": "3.0.1",
    "moment": "2.27.0",
    "roboto-fontface": "0.10.0",
    "rxjs": "6.6.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "9.0.1",
    "@angular-devkit/build-angular": "0.1000.3",
    "@angular-devkit/core": "10.0.3",
    "@angular-devkit/schematics": "10.0.3",
    "@angular-eslint/builder": "0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin": "0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin-template": "0.0.1-alpha.32",
    "@angular-eslint/template-parser": "0.0.1-alpha.32",
    "@angular/cli": "10.0.3",
    "@angular/compiler-cli": "10.0.4",
    "@angular/language-service": "10.0.4",
    "@briebug/cypress-schematic": "3.3.0",
    "@commitlint/cli": "9.1.1",
    "@commitlint/config-conventional": "9.1.1",
    "@cypress/webpack-preprocessor": "5.4.1",
    "@ngxs/devtools-plugin": "3.6.2",
    "@ngxs/logger-plugin": "3.6.2",
    "@types/jest": "26.0.4",
    "@types/jsdom": "16.2.3",
    "@types/marked": "1.1.0",
    "@types/node": "14.0.23",
    "@typescript-eslint/eslint-plugin": "3.6.1",
    "@typescript-eslint/parser": "3.6.1",
    "codelyzer": "6.0.0",
    "cypress": "4.10.0",
    "eslint": "7.4.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-compat": "3.8.0",
    "eslint-plugin-deprecation": "1.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-rxjs": "0.0.2-beta.28",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "eslint-watch": "7.0.0",
    "firebase": "7.16.0",
    "firebase-admin": "9.0.0",
    "firebase-functions": "3.8.0",
    "firebase-tools": "8.6.0",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "jest-preset-angular": "8.2.1",
    "prettier": "2.0.5",
    "prettier-stylelint": "0.4.2",
    "sort-json": "2.0.0",
    "sort-package-json": "1.44.0",
    "stylelint": "13.6.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-prettier": "1.1.2",
    "ts-jest": "26.1.2",
    "ts-loader": "8.0.1",
    "ts-node": "8.10.2",
    "typescript": "3.9.7",
    "web-animations-js": "2.3.2",
    "webpack-bundle-analyzer": "3.8.0"
  },
  "engines": {
    "node": ">=12.16.1",
    "npm": ">=6.14.4"
  }
}
